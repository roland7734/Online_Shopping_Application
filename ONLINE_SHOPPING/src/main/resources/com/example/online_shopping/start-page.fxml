<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.online_shopping.Controllers.StartPageController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="345.0" layoutY="125.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@BackGroundImages/admin_icon.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="110.0" layoutY="125.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@BackGroundImages/user_icon.jpg" />
         </image>
      </ImageView>
      <Label layoutX="257.0" layoutY="49.0" text="Login As">
         <font>
            <Font name="Cambria Bold" size="22.0" />
         </font>
      </Label>
      <Button fx:id="userButton" layoutX="147.0" layoutY="302.0" mnemonicParsing="false" onAction="#onUserButtonClick" styleClass="add-btn" stylesheets="@Styles/Styles.css" text="User">
         <font>
            <Font name="Cambria Bold" size="22.0" />
         </font>
      </Button>
      <Button fx:id="adminButton" layoutX="372.0" layoutY="302.0" mnemonicParsing="false" onAction="#onAdminButtonClick" styleClass="add-btn" stylesheets="@Styles/Styles.css" text="Admin">
         <font>
            <Font name="Cambria Bold" size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
